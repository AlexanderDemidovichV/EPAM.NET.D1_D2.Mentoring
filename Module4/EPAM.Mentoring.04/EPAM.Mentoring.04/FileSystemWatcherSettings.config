<?xml version="1.0" encoding="utf-8" ?>
<FileSystemWatcherSettings CultureInfo="en-US">
  <Rules>
    <Rule FilePattern=".txt" DestinationFolder="c:\1" ActionToTakeWhenInputFileNameIsChanged="AddIndexNumber"/>
    <Rule FilePattern=".rtf" DestinationFolder="c:\2" ActionToTakeWhenInputFileNameIsChanged="AddMoveDate"/>
  </Rules>
  <Directories>
    <Directory Path="c:\3"></Directory>
  </Directories>
</FileSystemWatcherSettings>